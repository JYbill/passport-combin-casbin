{
  "extends": "./node_modules/mwts/",
  "ignorePatterns": [
    "node_modules",
    "dist",
    "test",
    "jest.config.js",
    "typings"
  ],
  "env": {
    "jest": true
  },
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "off",
    "prettier/prettier": [
      "warn",
      {
        "singleQuote": true,
        "printWidth": 120
      }
    ],
    "node/no-unpublished-import": "off"
  }
}